<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body class="flex flex-col">
    <%- include('sidebar') %>
    <div>
      <h1><%= title %></h1>
      <ul>
        <% itemList.forEach(function(item) { %>
            <li><a href='/products/<%= item.id %>'><%= item.name %></a></li>
        <% }); %>
        </ul>
    </div>
  </body>
</html>
